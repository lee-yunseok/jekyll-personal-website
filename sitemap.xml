---
layout: null
---
<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
    {% for page in site.pages %}
        <url>
            <loc>{{ site.github.url }}{{ page.url | prepend: site.baseurl }}</loc>
        </url>
    {% endfor %}
    {% for post in site.posts %}
        <url>
            <loc>{{ site.github.url }}{{ post.url | prepend: site.baseurl }}</loc>
        </url>
  {% endfor %}
</urlset>
